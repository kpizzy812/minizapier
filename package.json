{
  "name": "minizapier",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "pnpm --filter @minizapier/api db:generate",
    "db:migrate": "pnpm --filter @minizapier/api db:migrate",
    "db:push": "pnpm --filter @minizapier/api db:push",
    "db:studio": "pnpm --filter @minizapier/api db:studio"
  },
  "devDependencies": {
    "turbo": "^2.5.3"
  }
}
